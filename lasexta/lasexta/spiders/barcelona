<script language="javascript" type="text/javascript">
<!-- Hide from old browsers
function publicidadCintilloL(){
	//if ($('body') && $('body') && $('body').id!=='home'){
	//comprueba si el body no tiene id:
	if(!document.body.id)document.body.id='body_publi';
	//if (document.body &&  document.body.id!=='home'){
	var idbody=document.body.id;
	$(idbody).addClassName('body_cintillo_l');
	
	//$('home').writeAttribute('class', 'body_cintillo_l');
	
	//aplico el alto total de la web
	Event.observe(window, 'load', daArtura);
	//}
}

function daArtura(){
	//altura la maxima sera la del contenido de la web y el background de la misma 
	//el color se implementa xq en principio iba a ir a color sólido hasta el final de la pagina
	var alturaCintilloll = $('background-web').getHeight();
	$('cintilloll').setStyle({
		display:'block',
		backgroundColor:'#fff',
		height:($('background-web').getHeight()+90)+'px'}
	);
}

//VIS=1; //Modo Prueba
if ((INTER != ''))
{
	if ((INTER > 0) && (INTER < 10))
	{
		new publicidadCintilloL;
	}
	else
		INTER = '';
}
// End Hide -->
</script>

